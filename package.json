{
  "name": "@prepawin/graph",
  "version": "1.0.1",
  "private": true,
  "dependencies": {
    "glamor": "^2.20.40",
    "katex": "^0.11.1",
    "mathjs": "^6.6.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env NODE_ENV=production babel ./src/lib --out-dir ./build --copy-files && yarn build:config-package",
    "build:config-package": "node ./scripts/config-package.js",
    "prebuild": "rimraf build",
    "release": "yarn build && npm publish build --access public",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "^7.0.2",
    "fs-extra": "^9.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
